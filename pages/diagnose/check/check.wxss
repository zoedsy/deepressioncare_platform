.branch{
  width: 55px;
  height: 3px;
  background-color:	#C0C0C0;
  transform: translate(70px,-20px);
}
.branched{
  width: 55px;
  height: 3px;
  background-color: cornflowerblue;
  transform: translate(70px,-20px);
}
.node{
  display: inline;
  margin-right: 40rpx;
  
}
.iconfont{
  margin-left: 30px;
}

.title{
  /* margin-right: 5px; */
  margin-left: 14px;
}

/* 病人个人信息 */
.info_box{
  width: 730rpx;
  height: 260rpx;
  /* color: rgb(61, 46, 46); */
  background-color: #fff;
  margin: 0 auto;
  margin-top: 20px;
  box-shadow: 0 0 10px #888888;
  padding: 2px;
  padding-top: 15px;
  border-radius:5px 5px 5px 5px;
  font-size: 20px;


}
.info{
  display: flex;
  padding: 7px;
  position: relative;
}
.Name,.Type,.Date{
  position: absolute;
  right: 15px;
  color: #888888;
}

/* 机器分析 */
.analyze_box{
  width: 730rpx;
  height: 260rpx;
  /* color: rgb(61, 46, 46); */
  background-color: #fff;
  margin: 0 auto;
  margin-top: 20px;
  box-shadow: 0 0 10px #888888;
  padding: 2px; 
  display: flex;
  border-radius:5px 5px 5px 5px;

}

.analyze0{
  flex:5;
  background-color: #37d27b;
  text-align: center;
  border-radius:5px 5px 5px 5px;
}
.rob_Result{
  text-align: center;
  line-height: 80px;
  font-size: 20px;
}
.accuracy{
  text-align: center;
  line-height: 0px;
  font-size: 24px;
}
.analyze1{
  flex:6;
  text-align: center;
}
.rob_Result1{
  text-align: center;
  line-height: 80px;
  font-size: 20px;
}
.result{
  text-align: center;
  line-height: 0px;
  font-size: 24px;
}
/* 结果查看 */
.result_box{
  width: 730rpx;
  height: 260rpx;
  /* color: rgb(61, 46, 46); */
  background-color: #37d27b;
  margin: 0 auto;
  margin-top: 20px;
  box-shadow: 0 0 10px #888888;
  text-align: center;
  line-height: 130px;
  border-radius:5px 5px 5px 5px;
  font-size: 20px;
}
