  width: 730rpx;
  height: 300rpx;
  background-color: #fff;
  align-items: center;
  justify-content: center;
  border: 1rpx  solid #DCDCDC;
  margin: 0 auto;
  /* margin-top: 10px; */
  /* padding-left: 38px; */
  
  box-shadow: 0 0 10px #888888;
  border-radius:5px 5px 5px 5px;
}
