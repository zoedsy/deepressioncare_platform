.big_box{
  width: 410px;
  height: 780px;
  margin: 0 auto;
  border: 1px solid #DCDCDC;
  box-shadow: 0 0 10px #888888;
  border-radius:5px 5px 5px 5px;

}

.box{
  width: 414px;
  height: 207px;
  background-color: #37d27b;
  border-radius: 0px 0px 414px 414px;
}

.title0{
 margin-top: 20px;
}
.step_box{
  width: 730rpx;
  height: 300rpx;
  background-color: rgba(255, 255, 255, .8);
  align-items: center;
  justify-content: center;
  border: 1rpx  solid #DCDCDC;
  margin: 0 auto;
  /* margin-top: 10px; */
  /* padding-left: 38px; */
  
  box-shadow: 0 0 10px #888888;
  border-radius:5px 5px 5px 5px;

  position: relative;
  top: 10%;
}
.step{
  display: flex;
  padding-top: 15px;
}

