.branch{
  width: 55px;
  height: 3px;
  background-color:	#C0C0C0;
  transform: translate(70px,-20px);
}
.branched{
  width: 55px;
  height: 3px;
  background-color: cornflowerblue;
  transform: translate(70px,-20px);
}
.node{
  display: inline;
  margin-right: 40rpx;
  
}
.iconfont{
  margin-left: 30px;
}

.title{
  /* margin-right: 5px; */
