}

/* 病人个人信息 */
.info_box{
  width: 730rpx;
  height: 260rpx;
  /* color: rgb(61, 46, 46); */
  background-color: #fff;
  margin: 40rpx auto;
  margin-top: 80rpx;
  box-shadow: 0 0 10px #888888;
  padding: 2px;
  padding-top: 15px;
  border-radius:5px 5px 5px 5px;
  font-size: 20px;


}
.info{
  display: flex;
  padding: 7px;
  position: relative;
}
.Name,.Type,.Date{
  position: absolute;
  right: 15px;
  color: #888888;
}

/* 机器分析 */
.analyze_box{
  width: 730rpx;
  height: 260rpx;
  /* color: rgb(61, 46, 46); */
  background-color: #fff;
  box-shadow: 0 0 10px #888888;
  padding: 2px; 
  display: flex;
  border-radius:5px 5px 5px 5px;
  margin: 20rpx auto;
}

.analyze_box image{
  height: 100%;
  width: 50%;
  opacity: 0.7;
}
.result{
  width: 50%;
  background-color: #e9ebf1;
}
.rob_Result{
  text-align: center;
  margin: 40px 0;
  font-size: 20px;
}
.rob_Result1{
  text-align: center;
  font-size: 20px;
}
.result{
  text-align: center;
  line-height: 0px;
  font-size: 24px;
}
/* 结果查看 */
.audio_box{
  width: 98%;
  height: 150px;
  margin: 0 auto;
  box-shadow: 0 0 5px #000;
  border-radius: 5px 5px 5px 5px;
  border: 1px solid#a2a2a2;
  margin: 25px 0;
  margin-left: 3px;
}
.audio_box image{
  width: 100%;
  height: 100%;
}

.tip{
