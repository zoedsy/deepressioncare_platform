.big_box{
  width: 410px;
  height: 780px;
  margin: 0 auto;
  border: 1px solid #DCDCDC;
  box-shadow: 0 0 10px #888888;
  border-radius:5px 5px 5px 5px;

}

.box{
  width: 414px;
  height: 207px;
  background-color: #37d27b;
  border-radius: 0px 0px 414px 414px;
}

.title0{
 margin-top: 20px;
}
